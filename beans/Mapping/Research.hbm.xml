<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="beans.Research, beans" table="researches" lazy="false">

    <id name="ID" column="id" type="Int32">
      <generator class="identity"/>
    </id>
    <many-to-one name="Village" class="beans.Village, beans" column="village_id" not-null="true" fetch="join" cascade="none" />
    <property name="Type" column="type" type="beans.TroopType, beans" />
    <property name="Start" column="start_time" type="timestamp" />
    <property name="Level" column="upgrade_level" type="Int32" />
  </class>

</hibernate-mapping>