<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="beans.Build, beans" table="building" lazy="false">

    <id name="ID" column="id" type="Int32">
      <generator class="identity"/>
    </id>
    <many-to-one name="InVillage" class="beans.Village, beans" column="village_id" not-null="true" fetch="join" cascade="save-update" />
    <property name="Building" column="building" type="beans.BuildingType, beans" />
    <property name="Start" column="start_time" type="timestamp" />
    <property name="End" column="stop_time" type="timestamp" />
  </class>

</hibernate-mapping>