<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  
  <class name="beans.Mail, beans" table="mail" lazy="false" discriminator-value="0">
    <id name="ID" column="id" type="Int32">
      <generator class="identity"/>
    </id>
    <many-to-one name="From" class="beans.Player, beans" lazy="proxy" column="[From]" not-null="true" cascade="none" />
    <many-to-one name="To" class="beans.Player, beans" lazy="proxy" column="[To]" not-null="true" cascade="none" />
    <property name="Time" column="send_time" type="timestamp"/>
    <property name="Title" column="title" type="String" />
    <property name="Detail" column="detail" type="String" />
    <property name="Unread" column="unread" type="Boolean" />
    <property name="Sender_delete" column="sender_delete" type="Boolean" />
    <property name="Reviece_delete" column="reviece_delete" type="Boolean" />
  </class>
</hibernate-mapping>