<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  
  <class name="beans.Report, beans" table="reports" lazy="false" discriminator-value="0">

    <id name="ID" column="id" type="Int32">
      <generator class="identity"/>
    </id>
    
    <many-to-one name="Owner" class="beans.Player, beans" lazy="proxy" column="owner" not-null="true" cascade="none" />
    <property name="Time" column="create_time" type="timestamp"/>
    <property name="Title" column="title" type="String" />
    <property name="Unread" column="unread" type="Boolean" />
    <property name="Type" column="type" type="beans.ReportType, beans" />
    <many-to-one cascade="all" class="beans.Text, beans" column="text_id" lazy="proxy" fetch="join" name="Description" />
    
  </class>

</hibernate-mapping>